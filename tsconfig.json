{
	"compilerOptions": {
		"declaration": true,
		"declarationDir": "dist",
		"outDir": "dist",
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"target": "ES6"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
