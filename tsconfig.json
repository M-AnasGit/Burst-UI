{
	"compilerOptions": {
		"declaration": true,
		"declarationDir": "dist",
		"outDir": "dist",
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"rootDir": "src",
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
